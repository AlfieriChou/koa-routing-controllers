{
  "name": "koa-routing-controllers",
  "version": "1.0.0",
  "description": "koa routing controllers",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec 'ts-node --cache-directory .tscache' src/server.ts",
    "start": "NODE_ENV=production ts-node src/server.ts",
    "build": "tsc: build - tsconfig.json",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create -n Migration",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfieriChou/koa-routing-controllers.git"
  },
  "keywords": [
    "koa"
  ],
  "author": "AlfieriChou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlfieriChou/koa-routing-controllers/issues"
  },
  "homepage": "https://github.com/AlfieriChou/koa-routing-controllers#readme",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "bcrypt": "^3.0.2",
    "class-validator-jsonschema": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.6.1",
    "koa-bodyparser": "^4.2.1",
    "koa-jwt": "^3.5.1",
    "koa-logger": "^3.2.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.4.0",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "nunjucks": "^3.1.4",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "routing-controllers-openapi": "^1.4.2",
    "ts-httpexceptions": "^3.1.0",
    "tslib": "^1.9.3",
    "typeorm": "^0.2.8"
  },
  "devDependencies": {
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.33",
    "nodemon": "^1.18.7",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.30",
    "typescript": "^3.1.6"
  }
}
